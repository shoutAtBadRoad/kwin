<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.kwin.db.test.user.mapper.GreenhouseMapper">

    <select id="getGreById" resultType="com.kwin.db.test.user.entity.Greenhouse">
        select * from WarnGreenhouse where id=#{id}
    </select>
    <select id="insertGre">
        insert into WarnGreenhouse(id,date,context)values (#{id},#{date},#{context})
    </select>
    <select id="getAllGre" resultType="com.kwin.db.test.user.entity.Greenhouse">
        select * from WarnGreenhouse
    </select>
    <select id="deleteGreById">
        delete from WarnGreenhouse where id=#{id}
    </select>
</mapper>